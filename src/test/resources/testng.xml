<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="LabStation">
    <listeners>
        <listener class-name="com.biit.labstation.logger.TestListener"/>
    </listeners>

    <test name="LabStation" enabled="true" preserve-order="true">
        <groups>
            <run>
                <include name="userManagerDefaultData"/>
                <include name="login"/>
            </run>
        </groups>
        <classes>
            <class name="com.biit.labstation.tests.LoginIT"/>
            <class name="com.biit.labstation.tests.UserManagerIT"/>
        </classes>
    </test>
</suite>
