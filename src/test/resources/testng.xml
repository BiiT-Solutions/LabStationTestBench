<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="LabStation">
    <listeners>
        <listener class-name="com.biit.labstation.logger.TestListener"/>
    </listeners>

    <test name="LabStation" enabled="true" parallel="none" thread-count="1">
        <groups>
            <run>
                <include name="userManagerDefaultData"/>
                <exclude name="userManagerBasicUserPermissions"/>
                <exclude name="userManagerEditorUserPermissions"/>
                <exclude name="userManagerDuplications"/>
                <exclude name="organization"/>
                <exclude name="validation"/>
                <exclude name="login"/>
                <exclude name="profiles"/>
                <exclude name="projects"/>
                <exclude name="cardgame"/>
                <exclude name="signUp"/>
                <exclude name="cadt"/>
                <exclude name="cadtDashboard"/>
                <exclude name="organizationAdmin"/>
                <exclude name="organizationAdminDashboard"/>
                <exclude name="orgAdminMetaviewer"/>
                <exclude name="activities"/>
            </run>
        </groups>
        <classes>
            <class name="com.biit.labstation.tests.usermanager.UserManagerTests"/>
            <class name="com.biit.labstation.tests.usermanager.UserManagerBasicUserPermissionsTests"/>
            <class name="com.biit.labstation.tests.usermanager.UserManagerEditorUserPermissionsTests"/>
            <class name="com.biit.labstation.tests.usermanager.UserManagerDuplicationsTests"/>
            <class name="com.biit.labstation.tests.usermanager.SignUpTests"/>
            <class name="com.biit.labstation.tests.usermanager.OrganizationsTests"/>
            <class name="com.biit.labstation.tests.usermanager.ValidationTests"/>
            <class name="com.biit.labstation.tests.usermanager.OrganizationAdminTests"/>
            <class name="com.biit.labstation.tests.LoginIT"/>
            <class name="com.biit.labstation.tests.profilematcher.ProfilesTests"/>
            <class name="com.biit.labstation.tests.cardgame.CardGameTests"/>
            <class name="com.biit.labstation.tests.dashboard.CadtDashboardTests"/>
            <class name="com.biit.labstation.tests.dashboard.OrganizationAdminDashboardTests"/>
            <class name="com.biit.labstation.tests.dashboard.UserActivitiesTests"/>
            <class name="com.biit.labstation.tests.profilematcher.OrganizationAdminMetaviewerTests"/>
        </classes>
    </test>
</suite>
